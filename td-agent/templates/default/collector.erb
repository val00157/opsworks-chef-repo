# /etc/td-agent/td-agent.conf
<source>
  type forward
  port 24224
</source>

<match *.**>
  type copy

  <store>
    type stdout
  </store>
  <store>
    type elasticsearch
    host localhost
    port 9200
    logstash_format true
    logstash_prefix tomcat_access
    logstash_dateformat %Y%m
  </store>
</match>

