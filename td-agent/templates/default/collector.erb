# /etc/td-agent/td-agent.conf
<source>
  type forward
  port 24224
</source>

<match **>
  type forest
  subtype copy
  <tamplate>
    <store>
      type elasticsearch
      host localhost
      port 9200
      logstash_format true
      logstash_prefix ${tag}
      logstash_dateformat %Y%m
    </store>
<case adrequest>
    <store>
      type stdout
    </store>
</case>
  </tamplate>
</match>

